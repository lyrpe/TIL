# 131
과일 = ["사과","귤","수박"]

for 변수 in 과일:
    print(변수)

# 132
for 변수 in 과일:
    print("#####")

# 133, 134
for 변수 in ["A", "B", "C"]:
  print(변수)

# 135
for 변수 in ["A", "B", "C"]:
  b = 변수.lower()
  print("변환:", b)

# 136, 137

for 변수 in [10, 20, 30]:
    print(변수)

리스트 = [10, 20, 30]
for 변수 in 리스트:
    print(변수)

# 138
for 변수 in [10, 20, 30]:
  print(변수)
  print("-------")

# 139
print("++++")
for 변수 in [10, 20, 30]:
  print(변수)

# 140
for 변수 in [1, 2, 3, 4]:
  print("-------")
