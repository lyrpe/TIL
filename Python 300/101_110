# 101 : bool 타입 -> true, false

# 102
print(3 == 5)

# 103 
print(3 < 5)

# 104
x = 4
print(1 < x < 5)

# 105
print((3 == 3 and (4 != 3)))

# 106 => 와 같은 연산자는 지원하지 않는다 (주의)

# 107
if 4 < 3 :
    print("hello world")

# 108
else :
    print("Hi, there")


# 109
if True :
    print("1")
    print("2")
else :
    print("3")
print("4")

# 110
if True :
    if False:
        print("1")
        print("2")
    else:
        print("3")
else :
    print("4")
print("5")
