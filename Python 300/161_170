    print(num / 10, num)

# 166
for i in range(1, 10) :
    print (3, "x", i, " = ", 3 * i)

# 167
num = 3
for i in range(1, 10, 2) :
    print (num, "x", i, " = ", num * i)

for i in range(1, 10) :
    if i % 2 == 1 :
        print (num, "x", i, " = ", num * i)

# 168
sum_num = 0
for i in range(1,11):   
    sum_num += i

print(sum_num)

